// REDIRECT TO DYNAMIC PAGE
import { getCollection } from 'astro:content';

// Get the year from CMS and redirect to dynamic URL
const fairYears = await getCollection('fair-years');
const year2026Data = fairYears.find(item => item.id === '2026');
const dynamicYear = year2026Data?.data?.year || 2026;

console.log('ðŸ”„ REDIRECT: 2026 page redirecting to:', `${dynamicYear}-fuar-takvimi`);

// Redirect to dynamic URL
return Astro.redirect(`/tr/${dynamicYear}-fuar-takvimi/`);
